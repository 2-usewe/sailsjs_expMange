	<!--================ Banner Area =================-->
	<section class="banner_area" style="background-color: rgb(212, 94, 94);">
		<div class="banner_inner d-flex align-items-center">
			<div class="container">
				<div class="banner_content text-left">
					<h2>Account Details</h2>
					<div class="page_link">
						<a href="/users">Home</a>
						<a href="#accountdetails">Accountdetails</a>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!--================End Banner Area =================-->

	<!-- Start Appointment Area -->
	<section class="appointment-area" id="accountdetails">
		<div class="container">
			<div class="row justify-content-between align-items-center appointment-wrap">
				<div class="col-lg-5 col-md-6 appointment-left">
					<h1>
						User: <span></span>
					</h1>
					
					<ul class="time-list">
						<li class="d-flex justify-content-between">
							<span>Account Name:</span>
							<span><%=foundAccount.accountname%></span>
						</li>
						<li class="d-flex justify-content-between">
							<span>Account Holdername</span>
							<span><%=username%></span>
						</li>
						<li class="d-flex justify-content-between">
							<span>Members:</span>
							<%foundAccount.members.forEach(function(members,index){%>
							<span><%=index+1%>.<%=members.name%><br>(<%=members.email%>)</span>
							<%})%>
						</li>
					</ul>

				</div>
                <div class="col-lg-6 col-md-6 pt-60 pb-60">
					<div class="appointment-right">
                        <h3 class="pb-20 text-center mb-20">Add Member</h3>
                        <div class="text-center">
                         <a href="/addmember/<%=foundAccount.id%>"><button class="main_btn text-uppercase">add</button></a>
                        </div>
                    </div>
                </div>
				
			</div>
		</div>
	</section>
	<!-- End Appointment Area -->

	<!--================ About Myself Area =================-->
	<section class="about_myself section_gap">
		<div class="container">
			<div class="row">
				<div class="col-lg-6 pr-0">
					<div class="about_img">
						<img class="img-fluid w-100" src="img/trens.jpg" alt="">
					</div>
				</div>

				<div class="col-lg-6 pl-0">
					<div class="about_box">
						<div class="section-title-wrap text-left">
							<h1>Transaction Details:</h1>
							
						</div>

						<div class="activity">
							<div class="row">
								<div class="col-lg-6 col-md-6">
									<div class="activity_box">
										<div>
											<i class="lnr lnr-database"></i>
										</div>
										<h3>$<span class="counter"><%=income-expense - transfer %></span> M</h3>
										<p>Total</p>
									</div>
								</div>

								<div class="col-lg-6 col-md-6">
									<div class="activity_box">
										<div>
											<i class="lnr lnr-book"></i>
										</div>
										<h3>$<span class="counter"><%= income %></h3>
										<p>income</p>
									</div>
								</div>
								<div class="col-lg-6 col-md-6">
									<div class="activity_box">
										<div>
											<i class="lnr lnr-users"></i>
										</div>
										<h3>$<span class="counter"><%= expense + transfer %></h3>
										<p>expense</p>
									</div>
								</div>
								<div class="col-lg-6 col-md-6">
									<div class="activity_box">
										<div>
											<i class="lnr lnr-users"></i>
										</div>
										<h3>$<span class="counter"><%= transfer %></h3>
										<p>transfer</p>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!--================ End About Myself Area =================-->

	
